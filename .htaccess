RewriteEngine On

# Only rewrite if the request is not a real file or directory
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite /folder/file to /folder/file.php
RewriteRule ^(.+)$ $1.php [L]

# Custom 404 page
ErrorDocument 404 /feapp/frontend/Authentication/404.php